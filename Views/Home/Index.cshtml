        <div class="portfolio-gallery" style="width:100vw; margin:0;">
            @{
                var pfImageFolder = "images/portfolio";
                var pfServerPath = System.IO.Path.Combine(System.IO.Directory.GetCurrentDirectory(), "wwwroot", pfImageFolder);
                var pfImages = System.IO.Directory.Exists(pfServerPath) ?
                    System.IO.Directory.GetFiles(pfServerPath)
                        .Select(f => System.IO.Path.GetFileName(f)).ToList() : new List<string>();
                var pfColumns = 4;
                var pfImagesPerColumn = (int)Math.Ceiling(pfImages.Count / (double)pfColumns);
            }
            <div class="portfolio-columns">
                @for (int col = 0; col < pfColumns; col++) {
                    <div class="portfolio-column">
                        @for (int i = col * pfImagesPerColumn; i < Math.Min((col + 1) * pfImagesPerColumn, pfImages.Count); i++) {
                            var img = pfImages[i];
                            <div class="portfolio-item" data-category="@img.ToUpper()">
                                <img src="/@pfImageFolder/@img" alt="Portfolio Image" class="portfolio-img" />
                            </div>
                        }
                    </div>
                }
            </div>
        </div>
    </div>
